@inproceedings{ WOS:000223712000011,
Author = {Kristensen, SP},
Editor = {{Mander, U and Antrop, M}},
Title = {{Changing the landscape: landscape activities undertaken by different
   types of farmers in two parishes in Denmark between 1987 and 1997}},
Booktitle = {{MULTIFUNCTIONAL LANDSCAPES, VOL III: CONTINUITY AND CHANGE}},
Series = {{ADVANCES IN ECOLOGICAL SCIENCES}},
Year = {{2003}},
Volume = {{16}},
Pages = {{223-242}},
Note = {{Conference on Multifunctional Landscapes, ROSKILDE, DENMARK, 2000}},
Abstract = {{The last 50 years of agricultural intensification has had a serious
   impact on the agricultural landscapes of Europe, resulting in a decrease
   in landscape heterogeneity and habitat quality. Increasing awareness of
   the negative environmental impact of industrialised agriculture during
   the past two decades has led to the protection of many vulnerable areas.
   Several agri-environmental programmes have been implemented which
   provide economic incentives for farmers to improve habitat conditions.
   This paper investigates landscape changes undertaken by farmers on their
   land in a study area in central Jutland, Denmark in a ten-year period,
   in order to determine the type and magnitude of fanner involvement in
   landscape activities. The results indicate that more than half of the
   farmers were involved in landscape activities between 1987 and 1997.
   Hedgerow planting and removal and digging of ponds were the most
   important activities in terms of proportion of farmers involved.
   Planting of Christmas trees and conversion of agricultural land into
   permanent grassland were the most significant activities in terms of
   area concerned. Few full-time farmers were engaged in activities which
   required the conversion of land to non-agricultural land use. Many
   landscape activities were financially supported by public subsidies
   which therefore appear to be quite important. Very few farmers had
   entered into management agreements under the EU 2078/92 programme.}},
